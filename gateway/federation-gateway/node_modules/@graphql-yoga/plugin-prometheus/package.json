{
  "name": "@graphql-yoga/plugin-prometheus",
  "version": "6.10.1",
  "description": "Prometheus plugin for GraphQL Yoga.",
  "peerDependencies": {
    "graphql": "^15.2.0 || ^16.0.0",
    "prom-client": "^15.0.0",
    "graphql-yoga": "^5.15.1"
  },
  "dependencies": {
    "@envelop/prometheus": "^13.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/graphql-hive/graphql-yoga.git",
    "directory": "packages/plugins/prometheus"
  },
  "author": "Arda TANRIKULU <ardatanrikulu@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "typings": "typings/index.d.ts",
